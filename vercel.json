{
    "version": 2,
    "builds": [
      { "src": "package.json", "use": "@now/next" }
    ],
    "functions": {
      "postbuild": {
        "runtime": "now-node@16",
        "memory": 128,
        "includeFiles": ["prisma/schema.prisma"],
        "environment": {
          "NEXT_TELEMETRY_DISABLED": "1"
        }
      }
    }
  }
  